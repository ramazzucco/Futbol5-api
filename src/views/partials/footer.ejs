<footer>
    <div class="row bg-success mt-5 p-3">
        <div class="contacto col-12 col-md-6 col-lg-6 mt-3 text-center my-auto">
            <div class="social-networks d-flex justify-content-around mb-5 h-25">
                <% for(element of footer.redessociales) { %>
                    <a href="https://<%= element.url %>" target="_blank" rel="noopener noreferrer">
                        <i class="fab fa-<%= element.nombre %> fa-3x p-2"></i>
                    </a>
                <% } %>
            </div>
            <div class="info h-75">
                <h6 class="text-uppercase">Contacto</h6>
                <ul class="border-top border-bottom border-dark text-capitalize py-3">
                   <% for(element of footer.contacto) { %>
                        <li><%= element.title %>: <%= element.data %> </li>
                    <% } %>
                </ul>
            </div>
        </div>
        <div class="map col-12 col-md-6 col-lg-6 my-3">
            <iframe title="our location on the map"
                src="https://www.google.com/maps/embed?pb=!1m16!1m12!1m3!1d1674.072023025537!2d-60.636307608001246!3d-32.94720614091602!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m1!1m0!5e0!3m2!1ses!2sar!4v1604933086880!5m2!1ses!2sar" frameBorder="0"  width= "100%"  height= 300 allowFullScreen="" aria-hidden=false tabIndex="0"></iframe>
        </div>
    </div>
</footer>